  "custom": {
    "modmenu": {
      "links": {
        "modmenu.brilliant": "https://www.brilliantw.net/",
        "modmenu.discord": "https://discord.gg/5MHGpAFGEN",
        "modmenu.github": "https://github.com/BrilliantTeam/Brillitant-Chathud-Mod"
      }
